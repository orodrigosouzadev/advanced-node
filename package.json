{
  "name": "advanced-node",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Rodrigo Souza <rodrigo@rodrigosouza.dev>",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:dev": "rimraf dist && mkdir dist && cp .env ./dist/.env && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "test:fb-api": "yarn test -c ./jest.integration.config.js ./tests/external/facebook-api.test.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "pg-mem": "1.9.11",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.29"
  }
}
